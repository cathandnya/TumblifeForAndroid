<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
  <PreferenceCategory android:title="@string/setting_title_account">
    <PreferenceScreen
      android:key="@string/login_key"
      android:title="@string/login_title"
      android:summary="" />
  </PreferenceCategory>
  <PreferenceCategory android:title="@string/setting_title_setting">
    <CheckBoxPreference
      android:key="@string/setting_usessl_key"
      android:title="@string/setting_usessl_title"
      android:summary="@string/setting_usessl_summary"
      android:defaultValue="@string/setting_usessl_default" />
    <CheckBoxPreference
      android:key="@string/setting_quickpost_key"
      android:title="@string/setting_quickpost_title"
      android:summary="@string/setting_quickpost_summary"
      android:defaultValue="@string/setting_quickpost_default" />
    <CheckBoxPreference
      android:key="@string/setting_usepin_key"
      android:title="@string/setting_usepin_title"
      android:summary="@string/setting_usepin_summary"
      android:defaultValue="@string/setting_usepin_default" />
    <ListPreference
      android:key="@string/setting_pinaction_key"
      android:title="@string/setting_pinaction_title"
      android:summary="@string/setting_pinaction_summary"
      android:entries="@array/setting_pinaction_items"
      android:entryValues="@array/setting_pinaction_items"
      android:defaultValue="@string/setting_pinaction_default" />
    <PreferenceScreen
      android:key="@string/setting_hardkey_key"
      android:title="@string/setting_hardkey_title"
      android:summary="@string/setting_hardkey_summary" />
    <ListPreference
      android:key="@string/setting_dashboardtype_key"
      android:title="@string/setting_dashboardtype_title"
      android:entries="@array/setting_dashboardtype_items"
      android:entryValues="@array/setting_dashboardtype_items"
      android:defaultValue="@string/setting_dashboardtype_default" />
    <CheckBoxPreference
      android:key="@string/setting_skipminepost_key"
      android:title="@string/setting_skipminepost_title"
      android:defaultValue="@string/setting_skipminepost_default" />
    <CheckBoxPreference
      android:key="@string/setting_skiprebloggedpost_key"
      android:title="@string/setting_skiprebloggedpost_title"
      android:defaultValue="@string/setting_skiprebloggedpost_default" />
    <CheckBoxPreference
      android:key="@string/setting_skipphotos_key"
      android:title="@string/setting_skipphotos_title"
      android:defaultValue="@string/setting_skipphotos_default" />
    <CheckBoxPreference
      android:key="@string/setting_savephotos_key"
      android:title="@string/setting_savephotos_title"
      android:summary="@string/setting_savephotos_summary"
      android:defaultValue="@string/setting_savephotos_default" />
    <ListPreference
      android:key="@string/setting_sendto_key"
      android:title="@string/setting_sendto_title"
      android:summary="@string/setting_sendto_summary"
      android:entries="@array/setting_sendto_items_title"
      android:entryValues="@array/setting_sendto_items"
      android:defaultValue="@string/setting_sendto_default" />
    <CheckBoxPreference
      android:key="@string/setting_savestate_key"
      android:title="@string/setting_savestate_title"
      android:summary="@string/setting_savestate_summary"
      android:defaultValue="@string/setting_savestate_default" />
    <CheckBoxPreference
      android:key="@string/setting_clearcache_key"
      android:title="@string/setting_clearcache_title"
      android:summary="@string/setting_clearcache_summary"
      android:defaultValue="@string/setting_clearcache_default" />
    <PreferenceScreen
      android:key="@string/clearcache_key"
      android:title="@string/clearcache_title"
      android:summary="@string/clearcache_summary" />
  </PreferenceCategory>
</PreferenceScreen>